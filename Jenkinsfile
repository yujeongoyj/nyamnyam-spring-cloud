pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                // 빌드 스크립트 추가
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
                // 테스트 스크립트 추가
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
                // 배포 스크립트 추가
            }
        }
    }
}
